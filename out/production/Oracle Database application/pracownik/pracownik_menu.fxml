            <Button fx:id="buttonAnuluj" disable="true" layoutX="-1.0" layoutY="341.0" mnemonicParsing="false" onMouseClicked="#AnulujPressed" prefHeight="31.0" prefWidth="161.0" text="ANULUJ" />
            <ComboBox fx:id="textAdres" disable="true" layoutX="-1.0" layoutY="256.0" prefHeight="31.0" prefWidth="161.0" promptText="ADRES" />
