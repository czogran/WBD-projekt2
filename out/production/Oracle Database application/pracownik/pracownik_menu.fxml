                                            <cellValueFactory>
                                                <PropertyValueFactory property="dataNadania" />
                                            </cellValueFactory>
                                        </TableColumn>
                                        <TableColumn fx:id="columnRozpoczecie" prefWidth="119.0" text="ROZPOCZÄ˜CIE">
                                            <cellValueFactory>
                                                <PropertyValueFactory property="dataRozpoczecia" />
                                            </cellValueFactory>
                                        </TableColumn>
                                        <TableColumn fx:id="columnZakonczenie" prefWidth="89.0" text="ZAKONCZENIE">
                                            <cellValueFactory>
                                                <PropertyValueFactory property="dataZakonczenia" />
                                            </cellValueFactory>
                                        </TableColumn>
                                    </columns>
                                </TableView>
    </children>
