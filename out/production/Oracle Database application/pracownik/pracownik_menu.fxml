    <children>
                                <TableView fx:id="tableZadania" layoutX="-9.0" layoutY="-2.0" prefHeight="264.0" prefWidth="495.0" style="-fx-font-size: 8px">
                                    <columns>
                                        <TableColumn fx:id="columnIdZadania" prefWidth="93.0" text="ID ZADANIA ">
