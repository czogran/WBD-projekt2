            <Button fx:id="buttonUsun" layoutX="124.0" layoutY="20.0" mnemonicParsing="false" onMouseClicked="#DeleteClient" text="USUN" />
            <Button fx:id="buttonZaktualizuj" layoutX="208.0" layoutY="20.0" mnemonicParsing="false" onMouseClicked="#ActivateUpdateClient" text="ZAKTUALIZUJ" />
         </children>
      </Pane>
    </children>

